{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "Cybercoding",
  "license": "Uladzislau Klachkou",
  "scripts": {
    "postinstall": "if [ $NPM_CONFIG_PRODUCTION ]; then yarn run build; fi",
    "dev": "yarn run build && nodemon src/server/server.js --exec babel-node",
    "lint": "prettier --config .prettierrc --check 'src/**/*.{js,jsx}'",
    "build": "yarn run build:clean && yarn run build:client && yarn run build:server",
    "build:destroy": "rm -rf dist",
    "build:clean": "yarn run build:destroy && mkdir dist",
    "build:client": "webpack && cp -R public/ dist/",
    "build:server": "babel --ignore src/client -d ./dist ./src -s",
    "build:production": "ENV=production yarn run build"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "helmet": "^3.20.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-custom-properties": "^9.0.2",
    "postcss-custom-selectors": "^5.1.2",
    "postcss-loader": "^3.0.0",
    "postcss-utilities": "^0.8.4",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-router-transition": "^1.3.0",
    "react-spring": "^8.0.27",
    "react-typist": "^2.0.5",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.0.5",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.12.0",
    "copy-webpack-plugin": "^5.0.4",
    "image-webpack-loader": "^6.0.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "react-devtools": "^4",
    "terser-webpack-plugin": "^2.0.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
